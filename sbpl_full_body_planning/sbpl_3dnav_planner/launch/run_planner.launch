<launch>

  <node pkg="topic_tools" type="throttle" name="throttle_joint_states" args="messages joint_states 0.5" />

 <node launch-prefix="gdb" pkg="sbpl_two_arm_planner_node" type="runPlanner" name="run_planner" output="screen" respawn="false" >
   <!-- <node pkg="sbpl_two_arm_planner_node" type="runPlanner" name="run_planner" output="screen" respawn="false" > -->

    <rosparam command="load" file="$(find sbpl_two_arm_planner)/config/pr2_both_arms.yaml" />
    <param name="planner/motion_primitive_file" value="$(find sbpl_two_arm_planner)/config/pr2-26.mprim" />
    <param name="planner/left_arm_description_file" value="$(find sbpl_arm_planner)/config/pr2_left_arm.cfg" />
    <param name="planner/right_arm_description_file" value="$(find sbpl_arm_planner)/config/pr2_right_arm.cfg" />
    <remap from="joint_states" to="joint_states_throttle" />

    <param name="start_x" value="0.745" />
    <param name="start_y" value="0.0" />
    <param name="start_z" value="0.920" />
    <param name="start_r" value="0.0" />
    <param name="start_p" value="0.0" />
    <param name="start_y" value="0.0" />

    <param name="goal_x" value="0.745" />
    <param name="goal_y" value="0.0" />
    <param name="goal_z" value="0.820" />
    <param name="goal_r" value="0.0" />
    <param name="goal_p" value="0.0" />
    <param name="goal_y" value="0.0" />

    <param name="right_arm_pose_on_object_x" value="0.0" />
    <param name="right_arm_pose_on_object_y" value="-0.15" />
    <param name="right_arm_pose_on_object_z" value="0.0" />

    <param name="left_arm_pose_on_object_x" value="0.0" />
    <param name="left_arm_pose_on_object_y" value="0.15" />
    <param name="left_arm_pose_on_object_z" value="0.0" />
  
  </node>

</launch>
