<launch>

<!--  <node pkg="topic_tools" type="throttle" name="throttle_joint_states" args="messages joint_states 0.5" /> -->

  <node pkg="sbpl_two_arm_planner_node" type="requestPlan" name="request_plan" output="screen" respawn="false" >

    <param name="start_x" value="0.6" />
    <param name="start_y" value="0.30" />
    <param name="start_z" value="0.54" />
    <param name="start_r" value="0.0" />
    <param name="start_p" value="0.0" />
    <param name="start_y" value="0.0" />

    <param name="goal_x" value="0.7" />
    <param name="goal_y" value="-0.0" />
    <param name="goal_z" value="0.92" />
    <param name="goal_roll" value="0.0" />
    <param name="goal_pitch" value="0.0" />
    <param name="goal_yaw" value="0.0" />

    <param name="raise_torso" value="false" />

    <param name="attached_object_filename" value="$(find sbpl_two_arm_planner_node)/tray.txt" />
    
    <param name="known_objects_filename" value="$(find sbpl_two_arm_planner_node)/env/tabletop_tray.env" />

    <param name="remove_current_object" value="tray2" />

    <param name="right_arm_pose_on_object_x" value="0.00" />
    <param name="right_arm_pose_on_object_y" value="-0.15" />
    <param name="right_arm_pose_on_object_z" value="0.0" />

    <param name="left_arm_pose_on_object_x" value="0.0" />
    <param name="left_arm_pose_on_object_y" value="0.1" />
    <param name="left_arm_pose_on_object_z" value="0.00" />

  </node>

</launch>
