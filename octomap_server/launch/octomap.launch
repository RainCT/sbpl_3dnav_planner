<launch>
  <include file="$(find octomap_server)/launch/laser+stereo-perception.launch" />


 
  <node  machine="c2" name="octomap_server_combined" pkg="octomap_server" type="octomap_server_node" args="$(find octomap_server)/sushi.bt" > 
  <!-- <node  machine="c2" name="octomap_server_combined" pkg="octomap_server" type="octomap_server_node" launch-prefix="gdb -ex run -args" > -->
    <param name="resolution" value="0.025" /> 
    <param name="sensor_model/range" value="4.0" />

   <param name="sensor_model/hit" value= "0.8" />
   <param name="sensor_model/miss" value= "0.31" />
   <param name="sensor_model/min" value= "0.12" />
   <param name="sensor_model/max" value= "0.95" />

  	<param name="min_z_range" value="0.05" />
  	<param name="max_z_range" value="1.8" />
  	<param name="min_x_size" value="25.0" />
  	<param name="min_y_size" value="25.0" />


	<param name="pointcloud_min_z" value="0.10"/>
	<param name="pointcloud_max_z" value="1.80"/>
	<param name="filter_ground" value="true"/>

    <remap from="cloud_in" to="/narrow_stereo_textured/points_filtered_attached2" />
    
    
  </node>

  <rosparam command="load" ns="robot_description_planning" file="$(find octomap_server)/config/pr2_planning_description.yaml" />
  <node name="environment_server" pkg="planning_environment" type="environment_server" />

</launch>

