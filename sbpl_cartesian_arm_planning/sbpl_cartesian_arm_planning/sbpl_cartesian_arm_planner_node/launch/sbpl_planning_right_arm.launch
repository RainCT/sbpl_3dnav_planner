<launch>

  <node pkg="sbpl_cartesian_arm_planner_node" type="sbpl_cartesian_arm_planner_node" name="sbpl_right_arm" output="screen" respawn="false" >

    <rosparam command="load" file="$(find sbpl_cartesian_arm_planner)/config/pr2_right_arm.yaml" />

    <param name="planner/arm_description_file" value="$(find sbpl_cartesian_arm_planner)/config/pr2_right_arm.cfg" />
    <param name="planner/motion_primitive_file" value="$(find sbpl_cartesian_arm_planner)/config/pr2-26.mprim" />
  </node>

</launch>
